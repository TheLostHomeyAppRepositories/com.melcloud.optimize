{
  "id": "get_entsoe_prices",
  "title": {
    "en": "Fetch ENTSO-E day-ahead prices"
  },
  "titleFormatted": {
    "en": "Fetch ENTSO-E day-ahead prices for [[zone]]"
  },
  "hint": {
    "en": "Returns hourly prices for the selected ENTSO-E price area."
  },
  "args": [
    {
      "name": "zone",
      "type": "text",
      "title": {
        "en": "Zone (ISO or EIC)"
      },
      "placeholder": {
        "en": "SE3 or 10YSE-3--------J"
      }
    }
  ],
  "tokens": [
    {
      "name": "prices_json",
      "type": "string",
      "title": {
        "en": "Prices JSON"
      },
      "example": "[{\"ts_iso_utc\":\"2024-01-01T00:00:00.000Z\",\"price_eur_per_kwh\":0.12}]"
    }
  ]
}
